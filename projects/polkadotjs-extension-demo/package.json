  "scripts": {
    "clean": "rm -rf dist/ && mkdir -p dist/assets",
    "compile": "tsc",
    "copy-assets": "cp -R src/assets dist/",
    "build": "yarn clean && yarn copy-assets && parcel build index.html --detailed-report",
    "dev": "yarn clean && yarn copy-assets && parcel index.html --https --no-cache"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.0.0-beta.1",
    "@parcel/transformer-typescript-tsc": "^2.0.0-beta.1",
    "@parcel/validator-typescript": "^2.0.0-beta-1",
    "@vue/compiler-sfc": "^3.0.5",
    "parcel": "^2.0.0-beta.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@polkadot/extension-dapp": "^0.36.1",
    "@substrate/smoldot-provider": "0.0.1",
    "primeflex": "^2.0.0",
    "primeicons": "^4.1.0",
    "primevue": "^3.2.0-rc.1",
    "regenerator-runtime": "^0.13.7",
    "vue": "^3.0.5",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0-rc.2"
  }
